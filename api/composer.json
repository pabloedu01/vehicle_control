{
    "name": "laravel/tunap",
    "type": "project",
    "description": "Tunap API.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "barryvdh/laravel-dompdf": "^1.0",
        "beyondcode/laravel-websockets": "^1.12",
        "fruitcake/laravel-cors": "^2.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "guzzlehttp/psr7": "^1.7",
        "intervention/image": "^2.7",
        "jenssegers/mongodb": "^3.8",
        "laravel/framework": "^8.54",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "exclude-from-classmap": [
            "vendor/superbalist/flysystem-google-storage/src/GoogleStorageAdapter.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files":[
            "app/Helpers/APIs.php",
            "app/Helpers/Validations.php",
            "app/Helpers/Models.php",
            "app/Helpers/Urls.php",
            "app/Helpers/Arrays.php",
            "app/Helpers/JWT.php",
            "app/Helpers/Texts.php",
            "app/Overrides/superbalist/GoogleStorageAdapter.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
